<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ApartmentController">
    <children>
        <GridPane layoutX="25.0" layoutY="22.0" prefHeight="292.0" prefWidth="266.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="128.0" minWidth="10.0" prefWidth="98.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0" prefWidth="168.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Apartment:" />
                <Label text="Apartment Num:" GridPane.rowIndex="1" />
                <Label text="Square feet:" GridPane.rowIndex="2" />
                <Label text="Bathrooms:" GridPane.rowIndex="3" />
                <Label text="Price:" GridPane.rowIndex="4" />
                <Label text="Updated:" GridPane.rowIndex="5" />
                <Label text="Administrator:" GridPane.rowIndex="6" />
                <Label text="Tenant:" GridPane.rowIndex="7" />
                <ComboBox fx:id="aptComboBox" onAction="#aptComboBox_ItemSelected" prefHeight="25.0" prefWidth="167.0" GridPane.columnIndex="1" />
                <TextField fx:id="aptNumTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="squareFeetTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="bathroomsTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="priceTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="updatedTextField" disable="true" editable="false" focusTraversable="false" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <ComboBox fx:id="adminComboBox" prefHeight="25.0" prefWidth="169.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                <ComboBox fx:id="tenantComboBox" prefHeight="25.0" prefWidth="167.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <Button fx:id="saveAptButton" mnemonicParsing="false" onAction="#saveAptButton_Clicked" prefHeight="25.0" prefWidth="129.0" text="Save Apartment" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                <Button fx:id="viewInvoiceDigButton" mnemonicParsing="false" onAction="#viewInvoiceDigButton_Clicked" text="View Invoices" GridPane.columnIndex="1" GridPane.rowIndex="9" />
            </children>
        </GridPane>
    </children>
</AnchorPane>